[user]
    name = YOUR_NAME
    email = YOUR_EMAIL@example.com
[core]
    editor = nano
    excludesfile = ~/.gitignore_global
    autocrlf = input
[color]
    ui = auto
[init]
    defaultBranch = main
[pull]
    rebase = false
[merge]
    conflictstyle = zdiff3
[rerere]
    enabled = true
[status]
    showUntrackedFiles = all

